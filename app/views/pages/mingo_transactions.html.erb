							<div id="content" class="span10">
								<!-- start: Content -->

								<div>
									<hr>
									<ul class="breadcrumb">
										<li>
											<a href="#">Home</a> <span class="divider">/</span>
										</li>
										<li>
											<a href="#">Mingoville Transactions Activity</a>
										</li>
									</ul>
									<hr>
								</div>
								<hr>
								<%= javascript_tag do %>

								window.lab4 = "<%=@sub[0]%> Months";
								window.lab5 = "<%=@sub[1]%> Months";
								window.lab6 = "<%=@sub[2]%> Months";
								window.country1 = new Array( "<%=@mingCountries1[0]%>"<%for x in 1..(@mingCountries1.size-1) do%>,"<%=@mingCountries1[x]%>"<%end%>);
								window.country2 = new Array( "<%=@mingCountries2[0]%>"<%for x in 1..(@mingCountries2.size-1) do%>,"<%=@mingCountries2[x]%>"<%end%>);
								window.country3 = new Array( "<%=@mingCountries3[0]%>"<%for x in 1..(@mingCountries3.size-1) do%>,"<%=@mingCountries3[x]%>"<%end%>);
								window.date1 = new Date( <%=@date1.to_time.to_i%> * 1000)
								window.date2 = new Date( <%=@date2.to_time.to_i%> * 1000)
								window.trans1 = <%= @mingTransactions1.to_json %>;
								window.trans2 = <%= @mingTransactions2.to_json %>;
								window.trans3 = <%= @mingTransactions3.to_json %>;
								<% end %>
								<!-- Transactions table -->
								<div class="box">
									<div class="box-header">
										<h2><i class="icon-list-alt"></i><span class="break"></span>Transactions Activity</h2>
										<div class="box-icon">
											<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
											<a href="#" class="btn-close"><i class="icon-remove"></i></a>
										</div>
									</div>
									<div class="box-content">
										<div id="stackchart" class="center" style="height:300px;"></div>
											<p id="hoverdata"><span id="clickdata"></span></p>
										<p class="stackControls center">
											<input class="btn" type="button" value="With stacking">
											<input class="btn" type="button" value="Without stacking">
										</p>

										<p class="graphControls center">
											<input class="btn-primary" type="button" value="Bars">
											<input class="btn-primary" type="button" value="Lines">
											<input class="btn-primary" type="button" value="Lines with steps">
										</p>
									</div>
								</div>

								<%= javascript_tag do %>
								window.days = <%= @date2 - @date1%>
								window.three = <%=@three%>;
								window.six = <%=@six%>;
								window.twelve = <%=@twelve%>;
								<% end %>


								<div class="box">
									<div class="box-header">
										<h2><i class="icon-list-alt"></i><span class="break"></span>Transactions</span></h2>
										<div class="box-icon">
											<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
											<a href="#" class="btn-close"><i class="icon-remove"></i></a>
										</div>
									</div>
									<div class="box-content">
										Total transactions: <span class="green"><%=@three + @six + @twelve%></span> 
										<br>  Three months: <span class="green"><%=@three%></span> 
										<br>  Six months: <span class="green"><%=@six%></span> 
										<br>  Twelve months: <span class="green"><%=@twelve%></span> 
										<br>  Total income: <span class="green"><%=@income / 100%> DKK</span>
										<div id="piechart" style="height:300px"></div>
									</div>

									
								

								</div>


									<!-- UPDATE CHARTS FORM -->

								<%= form_tag do %>			
 									From: <br><%= select_date @date1, :prefix => :start_date %><br>
									To:   <br><%= select_date @date2, :prefix => :end_date %><br>
									<%= submit_tag("Update Chart", :class => "btn btn-large btn-info") %>
								<% end %>

							<hr>

								</div><!--/row-->

								


																	
							</div><!--/#content.span10-->