							<div id="content" class="span10">
								<!-- start: Content -->

								<div>
									<hr>
									<ul class="breadcrumb">
										<li>
											<a href="#">Home</a> <span class="divider">/</span>
										</li>
										<li>
											<a href="#">Virtual World stats</a>
										</li>
									</ul>
									<hr>
								</div>
								<hr>


								<div class="row-fluid sortable">

									<div class="box span12">
										<div class="box-header" data-original-title>
											<h2><i class="icon-user"></i><span class="break"></span>Popular Activities</h2>
											<div class="box-icon">
												<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
												<a href="#" class="btn-close"><i class="icon-remove"></i></a>
											</div>
										</div>
										<div class="box-content">
											<table class="table table-striped table-bordered bootstrap-datatable datatable">
												<thead>
													<tr>
														<th class="floatleft">Name <i class="fa-icon-sort"></i></th>
														<th class="floatleft">Times played <i class="fa-icon-sort-down"></i></th>
														<th class="floatleft">Users playing <i class="fa-icon-sort"></i></th>
													</tr>
												</thead>   
												<tbody>
													<% @mingPopGames.each do |i| %>

													<tr>
														<td><%= i.name %>   </td>
														<td class="center"><%= i.plays %></td>
														<td class="center"><%= i.users %></td>
													</tr>											 
													<% end %>
												</tbody>
											</table>            
										</div>
									</div><!--/span-->
								</div>	<!--/row shortable-->	
								<div class="row-fluid sortable">
									<div class="box span12">
										<div class="box-header" data-original-title>
											<h2><i class="icon-user"></i><span class="break"></span>Popular Inventory Products</h2>
											<div class="box-icon">
												<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
												<a href="#" class="btn-close"><i class="icon-remove"></i></a>
											</div>
										</div>
										<div class="box-content">
											<table class="table table-striped table-bordered bootstrap-datatable datatable">
												<thead>
													<tr>
														<th class="floatleft">Name <i class="fa-icon-sort"></i></th>
														<th class="floatleft">Times purchased <i class="fa-icon-sort-down"></i></th>
													</tr>
												</thead>   
												<tbody>
													<% @mingProducts.each do |i| %>

													<tr>
														<td><%= i.name %>   </td>
														<td class="center"><%= i.times %></td>
													</tr>											 
													<% end %>
												</tbody>
											</table>            
										</div>
									</div><!--/span-->

								</div>	<!--/row shortable-->	


										<hr>							
							</div><!--/#content.span10-->